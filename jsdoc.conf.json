{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true,
      "default": {
        "outputSourceFiles" : true
      },
      "applicationName": "MovieDB Challenge"
    },
    "opts": {
      "destination": "./docs/",
      "encoding": "utf8",
      "private": true,
      "recurse": true,
      "template": "./node_modules/postman-jsdoc-theme"
    }
}
